<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ControllerPeriodicites">
  <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0">
         <top>
            <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="95.0" prefWidth="640.0" BorderPane.alignment="CENTER">
               <children>
                  <Label alignment="CENTER" text="Liste des Périodicités">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </top>
         <left>
            <VBox alignment="CENTER" prefHeight="190.0" prefWidth="320.0" spacing="20.0" BorderPane.alignment="CENTER">
               <children>
                  <TableView fx:id="TablePeriodicite" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                      <TableColumn prefWidth="319.0" sortable="false" text="Libellé" />
                    </columns>
                  </TableView>
               </children>
            </VBox>
         </left>
         <bottom>
            <VBox alignment="CENTER" prefHeight="50.0" prefWidth="640.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="btn_retour" mnemonicParsing="false" onAction="#click_btn_retour" text="Revenir à l'acceuil" />
               </children>
            </VBox>
         </bottom>
         <right>
            <VBox alignment="CENTER" prefHeight="190.0" prefWidth="320.0" spacing="20.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="btn_ajout" mnemonicParsing="false" onAction="#click_btn_gestion" text="Ajouter une périodicité" />
                  <Button fx:id="btn_modifier" mnemonicParsing="false" onAction="#click_btn_gestion" text="Modifier une périodicité" />
                  <Button fx:id="btn_suppr" mnemonicParsing="false" onAction="#click_btn_suppr" text="Supprimer une périodicité" />
               </children>
            </VBox>
         </right>
      </BorderPane>
  </children>
</VBox>
